<div class="language_post">
  <% javascript_jquery.each do |post| %>
    <div class="post_title"><%= "#{post["title"]}" %></div>
    <div class="post_url"><a href="<%= post["url"] %>"><%= "#{post["url"]}" %></a></div>
    <div class="post_desc"><%= "#{post["description"]}" %></div>
  </br>
  <% end %>
</div>
